<phpdox xmlns="http://phpdox.net/config">
	<project name="nokaut-api-kit" source="../src" workdir="./phpdox">
		<collector publiconly="false">
			<include mask="*.php" />
		</collector>

		<generator output="./">
			<build engine="html" enabled="true" output="api">
				<file extension="html" />
			</build>
		</generator>
	</project>
</phpdox>